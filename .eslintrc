{
    "rules": {
        "react/no-unescaped-entities": ["error", {"forbid": [">", "}"]}]
    },
    "globals": {
        "window": true,
        "document": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}